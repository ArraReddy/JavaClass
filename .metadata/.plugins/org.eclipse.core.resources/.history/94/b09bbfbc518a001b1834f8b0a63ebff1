package com.inheritence;

public class A {

	public int a;
	int b;
	protected int c;
	private int d;
	
	public void f1()
	{
		System.out.println("from f1");
	}
	
	void f2()
	{
		System.out.println("from f2");
		
	}
	protected void f3()
	{
		System.out.println("from f3");
	}
	
	private void f4()
	{
		System.out.println("from f4");
	}

}
